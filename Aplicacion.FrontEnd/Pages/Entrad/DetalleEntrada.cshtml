@page
@model Aplicacion.FrontEnd.Pages.DetalleEntradaModel
@{
}
<div class="row">
    <div class="col-auto p-3 mb-2 bg-secondary text-white">
<h2>Detalle Entrada</h2>
<div>
    
        <div>
            Codigo: @Model.EntradaDetalle.Id
        </div>
        <div>
            Fecha y Hora: @Model.EntradaDetalle.FechaHora
        </div>
        <div>
            Modelo Vehiculo: @Model.EntradaDetalle.Vehiculo.Modelo
        </div>
        <div>
            Placa Vehiculo: @Model.EntradaDetalle.Vehiculo.Placa
        </div>
        <div>
            Color Vehiculo: @Model.EntradaDetalle.Vehiculo.Color
        </div>
        <div>
            Cedula del Conductor: @Model.EntradaDetalle.Conductor.Cedula
        <div>
            Nombre del Conductor: @Model.EntradaDetalle.Conductor.Nombre @Model.EntradaDetalle.Conductor.Apellido
        </div>
        <div>
            Celular del Conductor: @Model.EntradaDetalle.Conductor.Celular
        </div>
        <div>
            Producto que Transporta: @Model.EntradaDetalle.Producto.Nombre 
        </div>
 
</div>
<div><br><a href="./ListaEntrada" class="btn btn-success">Menu Principal</a></div>
</div>
</div>